{
  "name": "@airalogy/aimd-recorder",
  "type": "module",
  "version": "1.0.0",
  "description": "AIMD editor Vue components and UI",
  "keywords": [
    "aimd",
    "markdown",
    "editor",
    "vue",
    "components"
  ],
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./components": {
      "types": "./src/components/index.ts",
      "import": "./src/components/index.ts"
    },
    "./composables": {
      "types": "./src/composables/index.ts",
      "import": "./src/composables/index.ts"
    },
    "./styles": {
      "import": "./src/styles/aimd.css",
      "default": "./src/styles/aimd.css"
    }
  },
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "files": [
    "dist",
    "src/styles"
  ],
  "scripts": {
    "type-check": "vue-tsc --noEmit",
    "build": "vite build",
    "dev": "vite build --watch"
  },
  "peerDependencies": {
    "@vueuse/core": "^13.5.0",
    "naive-ui": "2.41.0",
    "vue": "^3.5.17"
  },
  "dependencies": {
    "@airalogy/aimd-core": "workspace:*",
    "@airalogy/aimd-renderer": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vueuse/core": "^13.5.0",
    "naive-ui": "2.41.0",
    "sass": "^1.90.0",
    "typescript": "5.8.3",
    "unocss": "^0.58.3",
    "unplugin-auto-import": "^0.17.3",
    "unplugin-vue-components": "^0.27.4",
    "vite": "^7.1.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vue": "^3.5.17",
    "vue-tsc": "^3.0.6"
  }
}